@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>No privacy!</p>

<table class="table table-striped">
    @foreach (var claim in User.Claims)
    {
       
        <tr>
            <td>@claim.Type/td>
            <td>@claim.ValueType/td>
            <td>@claim.Value/td>
        </tr>
        
    }
</table>
